{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["cypress.config.ts", "cypress/**/*.ts"]
}
